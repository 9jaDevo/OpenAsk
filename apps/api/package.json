{
    "name": "@openask/api",
    "version": "1.0.0",
    "private": true,
    "description": "OpenAsk API server",
    "type": "module",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc -p tsconfig.build.json",
        "start": "node dist/index.js",
        "test": "vitest run",
        "lint": "eslint \"src/**/*.{ts,tsx}\" --ignore-pattern \"**/__tests__/**\"",
        "typecheck": "tsc --noEmit",
        "seed": "tsx scripts/seed.ts"
    },
    "dependencies": {
        "@google/generative-ai": "^0.21.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-oauth2-jwt-bearer": "^1.6.0",
        "express-rate-limit": "^7.1.5",
        "helmet": "^7.1.0",
        "mongoose": "^8.0.3",
        "pino": "^8.17.2",
        "pino-http": "^8.6.1",
        "pino-pretty": "^10.3.1",
        "sanitize-html": "^2.11.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@openask/config": "workspace:*",
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.0",
        "@types/sanitize-html": "^2.9.5",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "@vercel/node": "^5.4.0",
        "eslint": "^8.56.0",
        "mongodb-memory-server": "^9.1.5",
        "supertest": "^6.3.3",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3",
        "vitest": "^1.0.4"
    }
}