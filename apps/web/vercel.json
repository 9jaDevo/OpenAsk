{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "cd ../.. && npx pnpm@9.15.0 --filter web build",
    "devCommand": "pnpm dev",
    "installCommand": "cd ../.. && npx pnpm@9.15.0 install --no-frozen-lockfile",
    "framework": null,
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}